{
  "name": "yet",
  "description": "yet another framework",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./build/types/**/*.d.ts",
    "require": "./build/cjs/index.js",
    "import": "./build/esm/index.js",
    "default": "./build/esm/index.js"
  },
  "scripts": {
    "dev": "nodemon --watch src --watch test -e ts --exec \"npm run build && ts-node ./test/index.ts\"",
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build": "npm-run-all compile && node ./scripts/prepare-package-json",
    "release": "npm run build && changeset publish",
    "lint": "tsc"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "author": "natixco",
  "license": "MIT",
  "dependencies": {
    "colorette": "^2.0.19"
  }
}
